@model IEnumerable<WebApplication5.Models.Lab>
@{
    ViewData["Title"] = "Labs";
}
<h2>@ViewData["Title"]</h2>
<a asp-action="Create">Create new</a>
<table class="table">
    <tr>
        <th>Name</th>
        <th>Address</th>
        <th></th>
    </tr>
    @foreach(var l in Model)
    {
        <tr>
            <td>@l.Name</td>
            <td>@l.Address</td>
            <td><a asp-action="Edit" asp-route-id="@l.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@l.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@l.Id">Delete</a>
            </td>
        </tr>
    }
</table>